{
  "name": "Pear Programming",
  "version": "1.1",
  "description": "A pomodoro timer for pair programming",
  "manifest_version": 3,
  "author": "Erik Cox and Ryan Trontz",

  "browser_action": {
    "default_title": "Pair Programming",
    "default_popup": "timer.html"
  },
  "permissions": ["storage", "notifications"],
  "web_accessible_resources": ["icon48.png"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
